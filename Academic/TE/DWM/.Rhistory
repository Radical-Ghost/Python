x <- c(1, 2, 3, 4, 5)
y <- c(2, 3, 5, 7, 11)
height <- c(4, 6, 8, 12, 14)
values <- c(10, 20, 30, 40)
labels <- c("A", "B", "C", "D")
data <- rnorm(100)
plot(x, y, main="Plot Title", xlab="X-axis Label", ylab="Y-axis Label")
barplot(height, main="Bar Plot Title", xlab="X-axis Label", ylab="Y-axis Label", col="blue")
hist(data, main="Histogram Title", xlab="X-axis Label", col="green", breaks=10)
pie(values, labels, main="Pie Chart Title", col=rainbow(length(values)))
boxplot(data, main="Boxplot Title", ylab="Values", col="orange")
plot(x, y, main="Scatter Plot Title", xlab="X-axis Label", ylab="Y-axis Label", pch=19, col="red")
plot(x, y, type="l", main="Line Plot Title", xlab="X-axis Label", ylab="Y-axis Label", col="blue")
q()
